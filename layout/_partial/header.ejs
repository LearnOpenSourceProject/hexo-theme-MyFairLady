<header>
    <span id="forkongithub"><a href="https://github.com/yzzting/hexo-theme-MyFairLady">Fork me on GitHub</a></span>
    <div class="blog-header">
        <a href="<%- url_for() %>" class="logo-link"><%= theme.title %></a>
        <% var first = true
            for(var i in theme.menu) { %>
                <% if (first) { %>
        <ul class="nav nav-list">
        <li class="nav-list-item"><a href="<%- url_for(theme.menu[i]) %>"><%= i %></a></li>
        <% first = false %>

        <% } else { %>
        <li class="nav-list-item"><a href="<%- url_for(theme.menu[i]) %>"><%= i %></a></li>
        <% } %>
    <% } %>
    </ul>
    </div>
</header>
